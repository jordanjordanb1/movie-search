{
  "name": "movie-search",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://simplemoviesearchapp.herokuapp.com/",
  "scripts": {
    "client": "cd client && npm run start",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.0"
  }
}
